{
    "name": "webeweb/jquery-querybuilder-bundle",
    "type": "symfony-bundle",
    "description": "Integrate jQuery QueryBuilder with Symfony 2",
    "keywords": ["webeweb", "bundle", "symfony", "jquery", "querybuilder"],
    "homepage": "http://github.com/webeweb/jquery-querybuilder-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/expression-language": "^2.6|^3.0"
    },
    "support": {
        "issues": "http://github.com/webeweb/jquery-querybuilder-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\JQuery\\QueryBuilderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\JQuery\\QueryBuilderBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
